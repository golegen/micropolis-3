# script for testing wish before installation
TCLDEFAULT=/tmp/micropolis-master/micropolis-activity/src/tclx/tcldef
export TCLDEFAULT
if [ $# = 0 ]
then
    exec /tmp/micropolis-master/micropolis-activity/src/tclx/wish
else
    exec /tmp/micropolis-master/micropolis-activity/src/tclx/wish "$@"
fi
